<?xml version='1.0'?>
<ToolchainConfig>
  <binutils>
    <binutils_version>2.19</binutils_version>
    <libbfd>yes</libbfd>
    <libiberty>yes</libiberty>
  </binutils>
  <cc>
    <cpp>yes</cpp>
    <fortran>yes</fortran>
    <gcc_version>4.3.2</gcc_version>
    <java>yes</java>
  </cc>
  <clib>
    <clib_version>2.6.1</clib_version>
    <libc>glibc</libc>
    <threading_lib>nptl</threading_lib>
  </clib>
  <debug>
    <dmalloc>yes</dmalloc>
    <duma>yes</duma>
    <gdb>yes</gdb>
    <ltrace>yes</ltrace>
    <strace>yes</strace>
  </debug>
  <gmp_mpfr>
    <gmp_version>4.2.4</gmp_version>
    <mpfr_version>2.3.2</mpfr_version>
    <use_gmp_mpfr>yes</use_gmp_mpfr>
  </gmp_mpfr>
  <os>
    <kernel_version>2.6.26.8</kernel_version>
    <target_os>linux</target_os>
  </os>
  <target_options>
    <abi>none</abi>
    <endiannes>none</endiannes>
    <float>hardware</float>
    <target_arch>x86</target_arch>
    <target_variant>i686</target_variant>
  </target_options>
  <toolchain_options>
    <build_shared_lib>yes</build_shared_lib>
    <manufacturer>nptl</manufacturer>
    <sysroot>yes</sysroot>
  </toolchain_options>
  <tools>
    <libelf>yes</libelf>
    <sstrip>yes</sstrip>
  </tools>
</ToolchainConfig>
